<p id="notice"><%= notice %></p>

<h1>Listing Individual Reviews</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.profile.name %></td>
        <td><%= link_to 'Reviews', user.profile %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<%= link_to 'New Individual review', new_individual_review_path %>
